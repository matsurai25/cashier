.deals
  table.deals__list
    tr.deals__list__head
      td 日時
      td 金額
      td 内容
      td(v-if="statistics_f()") 情報
      //- td.deals__list__price 
      //-   |{{earnings()}}

    tr.deal(v-for="deal in deals")
      td
        span {{dispTime(deal.created)}}
      td.deals__list__price
        span {{deal.price}}
      td.deals__list__name
        span(v-for="item_id in deal.items", track-by="$index")
          {{findItem(item_id).name}}
      td.deals__list__statistics(v-if="statistics_f()")
        span(v-if="deal.statistics")
          span.deal-statistics-gender(data='{{deal.statistics.gender}}')
          span.deal-statistics-age(data='{{deal.statistics.age}}')
          span.deal-statistics-sample(data='{{deal.statistics.sample}}')
